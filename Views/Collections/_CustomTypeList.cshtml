@model IEnumerable<string>
@if (Model.Count() > 0)
{
    <ul class="list-group" style="min-height: 125px;">
        @foreach (var value in Model)
        {
            <li class="list-group-item">@value</li>
        }

    </ul>
}
else
{
    <div class="d-flex align-items-center justify-content-center" style="min-height: 125px;">
        Empty
    </div>
}
